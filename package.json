{
  "name": "model-deployment-service",
  "version": "0.0.1",
  "description": "Microservice to storage machine learning models and make predictions on demand.",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src/server.js \"",
    "build": "babel src -s -D -d dist",
    "start": "node dist/server.js",
    "prestart": "npm run -s build",
    "test": "jest src",
    "lint": "./node_modules/.bin/eslint \"**/*.js\"",
    "coveralls": "jest --coverage && cat ./tests/coverage/lcov.info | coveralls"
  },
  "repository": "antoniomdk/model-deployment-service",
  "author": "Antonio Molner Domenech <antoniomdk@hotmail.es>",
  "license": "GNU General Public License v3.0",
  "dependencies": {
    "@azure/storage-blob": "^10.5.0",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "dotenv": "^8.1.0",
    "express": "^4.13.3",
    "helmet": "^3.21.1",
    "method-override": "^3.0.0",
    "mongoose": "^5.7.1",
    "morgan": "^1.8.0",
    "onnxjs": "^0.1.7",
    "ramda": "^0.26.1",
    "resource-router-middleware": "^0.6.0",
    "swagger-ui-express": "^4.1.1",
    "winston": "^3.2.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/preset-env": "^7.6.0",
    "coveralls": "^3.0.6",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-ramda": "^2.5.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^24.9.0",
    "nodemon": "^1.9.2"
  }
}
